<!-- Header
============================================= -->
<header id="header" class="dark header-size-md" data-sticky-shrink="false">
    <div id="header-wrap">
        <div class="container">
            <div class="header-row">

                <!-- Logo
                ============================================= -->
                <div id="logo">
                    <a href="~/Home/Index" data-dark-logo="@Url.Content("~/website/images/logo.png")" class="standard-logo"><img src="@Url.Content(" ~ /website/images/logo.png")" alt="Trusita Logo"></a>
                    <a href="~/Home/Index" data-dark-logo="@Url.Content("~/website/images/logo@2x.png")" class="retina-logo"><img src="@Url.Content(" ~ /website/images/logo@2x.png")" alt="Trusita Logo"></a>
                </div><!-- #logo end -->
                <div class="header-misc ml-lg-1 ml-md-1 ml-sm-auto">
                    <div id="top-search" class="header-misc-icon">
                        @*<li class="top-links-item top-bar-highlight">
                        *@
                        <div class="btn-group">
                            <button type="button" class="btn btn-outline-primary border-white text-white">
                                @SessionClass.GetUser(HttpContextAccessor.HttpContext).FirstName @SessionClass.GetUser(HttpContextAccessor.HttpContext).LastName
                            </button>
                            <button type="button" class="btn btn-outline-primary border-white  text-white dropdown-toggle dropdown-toggle-split" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <span class="sr-only">Toggle Dropdown</span>
                            </button>
                            <div class="dropdown-menu">
                                <a class="dropdown-item" href="~/User/Profile">@Resource.Profile</a>
                                <a class="dropdown-item" href="~/Accounts/Logout">@Resource.Logout</a>
                            </div>
                        </div>
                        @*
                            </li>*@
                        @*@if (HttpContextAccessor.HttpContext?.Session.GetString("LanguageName") == Resource.Arabic)
                            {
                            <a asp-controller="Home" asp-action="ChangeLanguage" asp-route-code="en-US" asp-route-flag="flag-icon-us" asp-route-direction="LTR" asp-route-name="@Resource.English" class="d-inline btn btn-outline-success">@Resource.English <img style="width: 30px;" src="~/images/flags/us.png" alt="@Resource.Image" /></a>
                            }
                            else
                            {
                            <a asp-controller="Home" asp-action="ChangeLanguage" asp-route-code="ar-EG" asp-route-flag="flag-icon-sa" asp-route-direction="RTL" asp-route-name="@Resource.Arabic" class="d-inline btn btn-outline-success">@Resource.Arabic <img style="width: 30px;" src="~/images/flags/sa.png" alt="@Resource.Image" /></a>
                            }*@
                    </div>
                </div>
                <div id="primary-menu-trigger">
                    <svg class="svg-trigger" viewBox="0 0 100 100"><path d="m 30,33 h 40 c 3.722839,0 7.5,3.126468 7.5,8.578427 0,5.451959 -2.727029,8.421573 -7.5,8.421573 h -20"></path><path d="m 30,50 h 40"></path><path d="m 70,67 h -40 c 0,0 -7.5,-0.802118 -7.5,-8.365747 0,-7.563629 7.5,-8.634253 7.5,-8.634253 h 20"></path></svg>
                </div>

                <!-- Primary Navigation
                ============================================= -->
                <nav class="primary-menu with-arrows mr-auto">

                    <ul class="menu-container">
                        <li class="menu-item">
                            <a class="menu-link" href="~/Home/Index"><div>@Resource.Home</div></a>
                        </li>
                        <li class="menu-item">
                            <a class="menu-link" href="~/UserAccount/Chalets"><div>@Resource.MyProperties</div></a>
                        </li>
                        <li class="menu-item">
                            <a class="menu-link" href="~/UserAccount/Chalets"><div>@Resource.Accounting</div></a>
                        </li>
                        <li class="menu-item">
                            <a class="menu-link" href="~/UserAccount/Chalets"><div>@Resource.Reports</div></a>
                        </li>
                    </ul>

                </nav>
                

            </div>
        </div>
    </div>
    <div class="header-wrap-clone"></div>
</header><!-- #header end -->
