<!DOCTYPE html>
<html dir="@HttpContextAccessor.HttpContext?.Session.GetString("LanguageDirection")" lang="ar-EG">
<partial name="_head" />
@RenderSection("css", required: false)
<body class="stretched">


    <!-- Document Wrapper
        ============================================= -->
    <div id="wrapper">

        <partial name="_headerAccount" />

        <section id="content">
            <div class="content-wrap">
                <div class="container">
                    
                    <div class="text-center" style="display: none" id="loading">
                        <span class="text-info spinner icons-spinner-5" aria-hidden="true"></span>
                        <h6>@Resource.PleaseWait</h6>
                    </div>
                    @if (TempData.ContainsKey("AlertType") && TempData.ContainsKey("AlertMessage"))
                    {
                        <div class="style-msg @TempData["AlertType"].ToString()msg">
                            <div class="sb-msg"><i class="icon-thumbs-up"></i><strong>@TempData["AlertTitle"].ToString()</strong> @TempData["AlertMessage"].ToString().</div>
                            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                        </div>
                    }
                    <div class="card rounded-top bg-white border-0 w-100 shadow p-3">
                        <div class="row">
                            <div class="sidebar border-right col-lg-2 pl-0 pr-0">
                                <div class="sidebar-widgets-wrap">
                                    <div class="widget widget_links clearfix">
                                        <ul>
                                            <li id="users" class="mlink py-3">
                                                <a href="~/Users/Index/">
                                                    <div>@Resource.UserManagement</div>
                                                </a>
                                            </li>
                                            <li id="cities" class="mlink py-3">
                                                <a href="~/Cities/Index">
                                                    <div>@Resource.CitiesManagement</div>
                                                </a>
                                            </li>
                                            @*<li id="banks" class="mlink py-3">
            <a href="~/Banks/Index">
                <div>@Resource.BankManagement</div>
            </a>
        </li>*@
                                            <li id="parameters" class="mlink py-3">
                                                <a href="~/Utilities/Index">
                                                    <div>@Resource.ParameterManagement</div>
                                                </a>
                                            </li>
                                            <li id="properties" class="mlink py-3">
                                                <a href="~/Properties/Index">
                                                    <div>@Resource.PropertiesManagement</div>
                                                </a>
                                            </li>
                                            <li id="accountingManagment" class="mlink py-3">
                                                <a href="~/Properties/Index">
                                                    <div>@Resource.AccountingManagment</div>
                                                </a>
                                            </li>
                                            <li id="reports" class="mlink py-3">
                                                <a href="~/Properties/Index">
                                                    <div>@Resource.Reports</div>
                                                </a>
                                            </li>
                                            <li id="settings" class="mlink py-3">
                                                <a href="~/Settings">
                                                    <div>@Resource.Settings</div>
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="postcontent col-lg-10">
                               

                                @RenderBody()
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </section><!-- #content end -->
        <footer id="footer" class="dark">
            <!-- Copyrights
    ============================================= -->
            <partial name="_footerAccount" />
        </footer>
    </div><!-- #wrapper end -->

